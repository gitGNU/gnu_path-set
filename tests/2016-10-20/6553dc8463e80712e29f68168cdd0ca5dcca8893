$ PATH_SET_TEST_INPUT=tests/files.txt
$ PATH_SET_TEST_POOL_SIZE=1K
$ PATH_SET_TEST_HASH_SIZE=1K
$ PATH_SET_TEST_NODE_SIZE=1K
