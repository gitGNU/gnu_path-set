$ PATH_SET_TEST_REHASH_SIZE=2
$ PATH_SET_TEST_REHASH_LOAD=NAN
$ PATH_SET_TEST_STRUCT_TYPE=path-trie
$ PATH_SET_TEST_SET_TYPE=linear-hash
$ PATH_SET_TEST_SEPARATORS='/'
$

