#
# # meta command:
# $ path-set-test \
# --input=tests/files2.txt \
# --expr='10 ** x' \
# --limits=1-6 \
# --pool-size=1K \
# --hash-size=1K \
# --node-size=1K \
# --rehash-size=2 \
# --rehash-load=NAN \
# --separators='/' \
# --repeat=10 \
# --sleep=100 \
# --path-trie \
# --linear-hash \
# --run-tests
#

$ date +'%Y-%m-%d %H:%M:%S.%N %z'
2016-10-20 17:02:51.765730015 +0300
$

$ ./path-set -C
